  "license": "GPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/stonehank/react-valine.git"
  },
  "homepage": "https://github.com/stonehank/react-valine#README",
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
